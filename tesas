        				$vk->sendMessage($peer_id, "$is_ResultAllText1\n\n$is_ResultAllText\n\n‚õî–í—Å–µ–≥–æ {$is_count} –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä-(–∞/–æ–≤)");
        	        	echo('ok');
        	            exit;
                    }
                    else
                    {
                    	$vk->sendMessage($peer_id, "‚õî –ù–µ—Ç –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä–æ–≤");
        	        	echo('ok');
        	            exit;
                    }
                }
                else if($args[0] == 'online')
                {
                	$is_ResultAllText1 = "üë• –°–ø–∏—Å–æ–∫ –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä–æ–≤ –æ–Ω–ª–∞–π–Ω:\n\n";
                	$PlayerOnlineAdm = $db->query("SELECT * from player_admin");
        			while ($RowsPlayerOnlineAdm = $PlayerOnlineAdm->fetch_assoc())
        			{
        				$is_PlayerIDOnline = $RowsPlayerOnlineAdm['PlayerID'];
        				$is_PlayerAppointedIDOnline = $RowsPlayerOnlineAdm['AppointedID'];
        				$is_PlayerAdmLevel = $RowsPlayerOnlineAdm['Level'];
        				$CheckOnlineNamePlayer = $db->query("SELECT * FROM players WHERE PlayerID = '$is_PlayerIDOnline' AND isOnline = '1'");
        				while ($rowsCheckOnline = $CheckOnlineNamePlayer->fetch_assoc()) 
                		{
        	                $is_NameAdmins = $rowsCheckOnline['Name'];
        
        	                if($is_PlayerAdmLevel <= 5)
        	                {
        						$is_ResultAllText = "üë§ –ê–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä @id{$is_PlayerAppointedIDOnline}({$is_NameAdmins})\n–£—Ä–æ–≤–µ–Ω—å: {$is_PlayerAdmLevel}\n\n$is_ResultAllText";
        					}
        	                else if($is_PlayerAdmLevel > 5)
        	                {
        						$is_ResultAllText = "üë§ –ê–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä @id{$is_PlayerAppointedIDOnline}({$is_NameAdmins})\n–£—Ä–æ–≤–µ–Ω—å: {$is_PlayerAdmLevel}(6)\n\n$is_ResultAllText";
        					}
        					$is_count ++;
        				}
        			}
        			if($is_count)
        			{
        				$vk->sendMessage($peer_id, "$is_ResultAllText1\n\n$is_ResultAllText\n\n‚õî–í—Å–µ–≥–æ –≤ —Å–µ—Ç–∏ {$is_count} –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä-(–∞/–æ–≤)");
        	        	echo('ok');
        	            exit;
                    }
                    else
                    {
                    	$vk->sendMessage($peer_id, "‚õî –ù–µ—Ç –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä–æ–≤ –≤ —Å–µ—Ç–∏");
        	        	echo('ok');
        	            exit;
                    }
                }				
            }
    	}
	    else if ($cmd === 'tab') {
    $onlinePlayers = $db->query("SELECT * FROM players WHERE isOnline = '1'");
    $onlinePlayersCount = $onlinePlayers->num_rows;

    $resultText = "üë• –°–ø–∏—Å–æ–∫ –∏–≥—Ä–æ–∫–æ–≤ –æ–Ω–ª–∞–π–Ω:\n\n";

    while ($player = $onlinePlayers->fetch_assoc()) {
        $playerName = $player['Name'];
        $playerGameID = $player['GameID'];
        $playerHours = $player['Hours'];
        $playerPing = $player['isPing'];

        $resultText .= "üë§ –ò–≥—Ä–æ–∫ $playerName [$playerGameID]\n";
        $resultText .= "–ß–∞—Å–æ–≤ –æ—Ç—ã–≥—Ä–∞–Ω–æ: $playerHours\n";
        $resultText .= "–ü–∏–Ω–≥: $playerPing\n\n";
    }

    $resultText .= "‚õî –í—Å–µ–≥–æ –≤ —Å–µ—Ç–∏ $onlinePlayersCount –∏–≥—Ä–æ–∫" . ($onlinePlayersCount !== 1 ? "–æ–≤" : "–∞");

    $vk->sendMessage($peer_id, $resultText);
    echo('ok');
    exit;
}
	    else if(in_array($cmd, ['help']))
	    {
			if($chat_id > 0)
			{
		    	$vk->sendMessage($peer_id, "
	    		–°–ø–∏—Å–æ–∫ –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –∫–æ–º–∞–Ω–¥:\n
	    		‚úÖ tab ‚Äî —Å–ø–∏—Å–æ–∫ –∏–≥—Ä–æ–∫–æ–≤ –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ –≤ —Å–µ—Ç–∏");
				echo('ok');
	            exit;
            }
        }
		else if(in_array($cmd, ['pm']))
	    {
	        if($chat_id == 8)
			{
    			$vk->sendMessage($peer_id, "–í –¥–∞–Ω–Ω–æ–º —á–∞—Ç–µ –Ω–µ –¥–æ—Å—Ç—É–ø–Ω–æ!");
    			echo('ok');
    			exit;
			}
			else if($chat_id == 10)
			{
			    if($args[0] == '')
                {
                    $vk->sendMessage($peer_id, "‚Äº –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: pm [–ù–æ–º–µ—Ä —Ä–µ–ø–æ—Ä—Ç–∞] [–°–æ–æ–±—â–µ–Ω–∏–µ]");
                    echo('ok');
                    exit;
                }
                if($args[1] == '')
                {
                    $vk->sendMessage($peer_id, "‚Äº –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ: pm [–ù–æ–º–µ—Ä —Ä–µ–ø–æ—Ä—Ç–∞] [–°–æ–æ–±—â–µ–Ω–∏–µ]");
                    echo('ok');
                    exit;
                }
				//if(!$args[21])
				//{
					$PlayerOnlineAdm = $db->query("SELECT * from players WHERE isOnline = '1'");
					while ($RowsPlayerOnlineAdm = $PlayerOnlineAdm->fetch_assoc())
					{
						$is_PlayerID = $RowsPlayerOnlineAdm['ID'];
						$is_PlayerName = $RowsPlayerOnlineAdm['Name'];
						$PlayerCheckKicks = $db->query("SELECT * from player_admin WHERE AppointedID = $id");
						while ($RowsAdm = $PlayerCheckKicks->fetch_assoc())
						{
							$is_AdminsGameID = $RowsAdm['PlayerID'];
							$PlayerAdmNames = $db->query("SELECT * from players WHERE ID = '$is_AdminsGameID'");
							while ($RowsAdmGames = $PlayerAdmNames->fetch_assoc())
							{	
								$is_AdminNames = $RowsAdmGames['Name'];
								//if($args[2] != '')
								//{
								  // $text = urlencode($args[1]);
								 //  $text = implode(' ', $args[1]);
								 $text = implode(' ', array_slice($args, 1));
									$vk->sendMessage($peer_id, "‚úÖ –°–æ–æ–±—â–µ–Ω–∏–µ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ.\n\n* –ò–≥—Ä–æ–∫—É: $is_PlayerName\n–¢–µ–∫—Å—Ç: $text");
									$db->query("UPDATE players SET PMCheck = '1', PMAdmin = '$is_AdminNames',  TextPM = '$text' WHERE PlayerID = '$args[0]' LIMIT 1");
									$db->query("UPDATE player_admin SET PM = PM + 1 WHERE PlayerID = '$is_AdminsGameID' LIMIT 1");
									echo('ok');
									exit;
								//}
							}
						}
					//}
					$vk->sendMessage($peer_id, "‚Äº –ò–≥—Ä–æ–∫–∞ –Ω–µ—Ç –≤ —Å–µ—Ç–∏ –∏–ª–∏ –æ–Ω –Ω–µ –∞–≤—Ç–æ—Ä–∏–∑–æ–≤–∞–ª—Å—è");
					echo('ok');
					exit;
				}
				//else
			//	{
				//	$vk->sendMessage($peer_id, "‚Äº –í –≤–∞—à–µ–º —Ç–µ–∫—Å—Ç–µ –±–æ–ª—å—à–µ 20 –ø—Ä–æ–±–µ–ª–æ–≤!");
				//	echo('ok');
				//	exit;
			//	}
			}
        }
    }
    default: echo('ok'), exit;
}
